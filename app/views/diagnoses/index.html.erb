<h1 class="text-4xl font-bold">過去の診断結果</h1>

<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-3">
  <% if @diagnoses.any? %>
    <%= render 'keyboards/keyboard', keyboards: @diagnoses %>
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-3">
    </div>
  <% else %>
      <div class="flex flex-col h-screen">
        <p class="mt-4 text-xl">診断結果はありません</p>
      </div>
  <% end %>
</div>
